# macOS执行
hdiutil convert -format UDRW -o alpine-extended-3.22.0-x86_64.img alpine-extended-3.22.0-x86_64.iso
sudo dd if=alpine-extended-3.22.0-x86_64.img.dmg of=/dev/rdisk2 bs=100m

