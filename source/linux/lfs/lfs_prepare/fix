grep "FAILED open or read" md5sum.log  | awk -F ":" '{print $1}' > wget-list-sysv_files

> wget-list-sysv.again
for file in `cat wget-list-sysv_files`; do
    grep $file wget-list-sysv | tee -a wget-list-sysv.again
done

wget --input-file=wget-list-sysv.again --continue --directory-prefix=$LFS/sources
